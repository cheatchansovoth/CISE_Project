const mongoose=require('mongoose');

const PendingBooksSchema= new mongoose.Schema(
    {
        title: String,
        authors: String,
        source: String,
        pubyear: Date,
        doi: String,
        claim: String,
        evidence: String,
    },
    {
        collection:'PendingBooks'
    }
);

mongoose.model('PendingBooks',PendingBooksSchema);