const mongoose=require('mongoose');

const userDetailsSchema= new mongoose.Schema(
    {
        name: String,
        email: String,
        password: String,
        isModerators: {type: Boolean,default:false},
        isAdmin: {type: Boolean,default:false}
    },
    {
        collection:'Usertbl'
    }
);

mongoose.model('Usertbl',userDetailsSchema);